{{#section 'medications'}}
<!-- Show the list of pre-existing conditions -->
<div>
    <ul>
      {{#each result}}
        <li class='toggle'>
          <p class='medLi'>
            &nbsp;&nbsp;{{MedicineName}}
            <button class="med-edit">Edit</button>
            <button class="med-delete">Delete</button>
          </p>
        </li>
        <br/>
        {{/each}}
    </ul>

</div>
<!-- Add new item Button -->
    <p><button id="addNewMedication">Add New Item</button></p>

{{/section}}
