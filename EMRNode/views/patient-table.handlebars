<div align="center">
<table id="patientTable" name="patientTable" border="1" cellpadding="3" cellspacing="0">
	<caption><p><u>:: Patient's List ::</u></p></caption>
{{result.1.SSN}}
	<thead>
    	<tr>
    		<th style="width:100px">SSN</th>
      		<th style="width:100px">First Name</th>
      		<th style="width:120px">Last Name</th>
      		<th style="width:120px">Birth Date</th>
      		<th style="width:60px">Gender</th>
      		<th style="width:300px">Current Status</th>
    	</tr>
  	</thead>
    {{#each result}}
		<tr onclick="/patient-details" style="cursor:pointer">

			<td style="width:100px">{{SSN}}</td>
			<td style="width:100px">{{First_Name}}</td>
			<td style="width:120px">{{Last_Name}}</td>
			<td style="width:120px">{{Birth_Date}}</td>
			<td style="width:60px">{{Gender}}</td>
			<td style="width:300px">{{Current_Status}}</td>
		</tr>
    {{/each}}
</table>
</div>
